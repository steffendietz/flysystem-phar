{
    "name": "steffendietz/flysystem-phar",
    "description": "Phar adapter for league/flysystem v2.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "flysystem",
        "phar"
    ],
    "autoload": {
        "psr-4": {
            "Steffendietz\\Flysystem\\Phar\\": "src/"
        }
