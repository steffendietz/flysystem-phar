        }
    },
    "authors": [
        {
            "name": "Steffen Dietz",
            "email": "ich@steffendietz.de",
            "homepage": "https://steffendietz.de"
        }
    ],
    "require-dev": {
        "league/flysystem-adapter-test-utilities": "^2.2",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    }
}
